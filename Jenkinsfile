pipeline {
  agent any
  stages {
    stage("Build") {
      steps {
        echo "Hello World!"
      }
    }
    stage("Test") {
      steps {
        echo "testing..."
      }
    }
    stage("Package") {
      steps {
        echo "packaging..."
      }
    }
    stage("Deploy") {
      steps {
        echo "Deploying..."
      }
    }
  }
}
